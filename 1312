import java.util.Scanner;

class Main{
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int n=sc.nextInt();
		int result=0;
		while(true) {
			a%=b;
			a*=10;
			result=a/b;
			n--;
			if(n==0) {
				System.out.println(result);
				break;
			}
		}
		
	}
}
